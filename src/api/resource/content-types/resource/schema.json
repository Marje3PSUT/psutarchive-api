{
  "kind": "collectionType",
  "collectionName": "resources",
  "info": {
    "singularName": "resource",
    "pluralName": "resources",
    "displayName": "Resource",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "files": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "material.metadata",
      "required": true
    },
    "material": {
      "type": "dynamiczone",
      "components": [
        "material.note-data",
        "material.exam-data"
      ],
      "required": true,
      "max": 1,
      "min": 1
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "resources"
    },
    "Ownership": {
      "type": "component",
      "repeatable": false,
      "component": "material.note"
    },
    "Disclaimer": {
      "type": "component",
      "repeatable": false,
      "component": "material.disclaimer"
    }
  }
}
