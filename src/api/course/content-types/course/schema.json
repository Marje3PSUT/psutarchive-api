{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "School": {
      "pluginOptions": {},
      "type": "enumeration",
      "enum": [
        "School of Computing Sciences",
        "School of Engineering",
        "School of Business Technology",
        "School of Graduate Studies & Scientific Research",
        "Elective Course"
      ],
      "required": true
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "course"
    },
    "CourseID": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "unique": true
    },
    "ArabicName": {
      "type": "string",
      "required": true
    },
    "quizzes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "course"
    }
  }
}
