{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "unique": true
    },
    "course_id": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 5,
      "minLength": 5,
      "regex": "[0-9]{5}"
    },
    "name_ar": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "recordings_url": {
      "type": "string"
    },
    "related_courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "alt_names": {
      "type": "component",
      "repeatable": true,
      "component": "courses.alt-names"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "courses"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "course"
    }
  }
